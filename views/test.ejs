<body data-open="click" data-menu="vertical-menu" data-col="2-columns"
  class="vertical-layout vertical-menu 2-columns  fixed-navbar">

  <!-- navbar-fixed-top-->


  <!-- ////////////////////////////////////////////////////////////////////////////-->


 

  <div class="app-content content container-fluid">
    <div class="content-wrapper">
      <div class="content-header row">
        
        
      </div>
      <div class="content-body"><!-- Basic form layout section start -->
        <section id="basic-form-layouts">



          <div class="row match-height">
            <div class="col-md-12">
              <div class="card">
                <div class="card-header">
                  <h4 class="card-title" id="basic-layout-tooltip">TEST</h4>
                  <a class="heading-elements-toggle"><i class="icon-ellipsis font-medium-3"></i></a>
                  <div class="heading-elements">
                    <ul class="list-inline mb-0">
                      <li><a data-action="collapse"><i class="icon-minus4"></i></a></li>
                      <li><a data-action="reload"><i class="icon-reload"></i></a></li>
                      <li><a data-action="expand"><i class="icon-expand2"></i></a></li>
                      <li><a data-action="close"><i class="icon-cross2"></i></a></li>
                    </ul>
                  </div>
                </div>
                <div class="card-body collapse in">
                  <div class="card-block">



                    <form class="form" action="/test" method="post" autocomplete="off">
                      <div class="form-body">

                        <input type="hidden" id="complaintinput1" class="form-control round" name="id"
                          value="<% if(typeof testToUpdate !== 'undefined'){ %> <%= testToUpdate.id %> <% } %>">
                        <% if (types && types.length) { %>
                          <div class="form-group">
                            <label for="id_test_type">id_test_type</label>
                            <select class="form-control" name="id" required>
                              <% types.forEach(function(type) { %>
                                <option value="<%= type.id %>">
                                  <%= type.testType_name %>
                                </option>
                                <% }); %>
                            </select>
                          </div>
                          <% } else { %>
                            <p>Aucun type de test trouvé</p>
                            <% } %>


                              <div class="form-group">
                                <label for="issueinput1">test_name</label>
                                <input type="text" id="issueinput1" class="form-control" placeholder="test_name"
                                  name="test_name" data-toggle="tooltip" data-trigger="hover" data-placement="top"
                                  data-title="Issue Title">
                              </div>

                              <div class="form-group">
                                <label for="issueinput8">test_description</label>
                                <textarea id="issueinput8" rows="5" class="form-control" name="test_description"
                                  placeholder="test_description" data-toggle="tooltip" data-trigger="hover"
                                  data-placement="top" data-title="Comments"></textarea>
                              </div>

                              <div class="form-group">
                                <label for="issueinput2">test_price</label>
                                <input type="text" id="issueinput2" class="form-control" placeholder="test_price"
                                  name="test_price" data-toggle="tooltip" data-trigger="hover" data-placement="top"
                                  data-title="Opened By">
                              </div>

                              <div class="row">
                                <div class="col-md-6">
                                  <div class="form-group">
                                    <label for="issueinput3">min_time_frame</label>
                                    <input type="text" id="issueinput3" class="form-control" name="min_time_frame"
                                      placeholder="min_time_frame" data-trigger="hover" data-placement="top"
                                      data-title="Date Opened">
                                  </div>
                                </div>
                                <div class="col-md-6">
                                  <div class="form-group">
                                    <label for="issueinput4">max_time_frame </label>
                                    <input type="text" id="issueinput4" class="form-control" name="max_time_frame 	"
                                      placeholder="man_time_frame" data-title="Date Fixed">
                                  </div>
                                </div>
                              </div>





                      </div>

                      <div class="form-actions">
                        <button type="button" class="btn btn-warning mr-1">
                          <i class="icon-cross2"></i> Cancel
                        </button>
                        <button type="submit" class="btn btn-primary">
                          <i class="icon-check2"></i> Save
                        </button>
                        <button class="btn btn-primary" name="action" value="Update">
                          <i class="icon-pencil2"></i> Update
                        </button>
                      </div>
                    </form>
                  </div>
                </div>
              </div>
            </div>


          </div>





          </form>
      </div>
    </div>
  </div>

  
  <div class="mt-3 p-4 bg-light text-dark" rounder>
    <h1>block</h1>

    <% if (tests && tests.length) { %>
      <table class="table">
        <thead>

          <tr>
            <th scope="col">id</th>
            <th scope="col">id_test_type</th>
            <th scope="col">test_name</th>
            <th scope="col">test_description</th>
            <th scope="col">test_price</th>
            <th scope="col">min_time_frame</th>
            <th scope="col">max_time_frame </th>
            <th scope="col">Action</th>

          </tr>
          <% tests.forEach (function (test) { %>
            <tr>
              <td scope="col">
                <%= test.id %>
              </td>

              <td scope="col">
                <%= test.id_test_type %>
              </td>
              <td scope="col">
                <%= test.test_name%>
              </td>
              <td scope="col">
                <%=test.test_description %>
              </td>
              <td scope="col">
                <%=test.test_price %>
              </td>
              <td scope="col">
                <%=test.min_time_frame %>
              </td>
              <td scope="col">
                <%=test.max_time_frame %>
              </td>
              <td>


                <a href="/test/update/<%= test.id %>" class="btn btn-primary"> <i class="icon-pencil2"></i></a>

                <a class="btn btn-danger" href="test/delete/<%= test.id %>"> <i class="icon-bin"></i></a>



              </td>
            </tr>
            <% }); %>
        </thead>

      </table>
      <% } else { %>
        <p>Aucun test trouvé</p>
        <% } %>
  </div>








  </section>
  <!-- // Basic form layout section end -->

